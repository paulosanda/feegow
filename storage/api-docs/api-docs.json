        "/api/prospection/new": {
            "post": {
                "tags": [
                    "Prospect"
                ],
                "summary": "Cadastra prospect",
                "description": "Cadastra prospect  - IMPORTANTE tenha certeza que cadastrou ao menos uma cateoria de source",
                "operationId": "Cadastra prospect",
                "requestBody": {
                    "description": "Cadastra prospecrt",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "specialty_id",
                                    "professional_id",
                                    "name",
                                    "cpf",
                                    "source_id",
                                    "birthdate"
                                ],
                                "properties": {
                                    "specialty_id": {
                                        "type": "integer",
                                        "example": 263
                                    },
                                    "professional_id": {
                                        "type": "integer",
                                        "example": 263
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Marcos Silva"
                                    },
                                    "cpf": {
                                        "type": "string",
                                        "example": "548.584.551-89"
                                    },
                                    "source_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "birthdate": {
                                        "type": "string",
                                        "example": "2001-05-20"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Source cadastrado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Source cadastrado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
